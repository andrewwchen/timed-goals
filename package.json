{
	"name": "timedgoals",
	"displayName": "TimedGoals",
	"description": "Julian George, Andrew Chen, and Alphonso Bradham's submission for HackDartmouth 2021",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:timedgoals.showTimer",
		"onCommand:timedgoals.viewUI",
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "timedgoals.viewUI",
				"title": "Timed Goals: View UI"
			},
			{
				"command": "timedgoals.showTimer",
				"title": "Timed Goals: Show Timer"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
