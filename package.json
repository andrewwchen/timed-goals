{
	"name": "timedgoals",
	"displayName": "TimedGoals",
	"description": "Julian George, Andrew Chen, and Alphonso Bradham's submission for HackDartmouth 2021",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:timedgoals.showTimer",
		"onCommand:timedgoals.viewUI",
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "timedgoals.viewUI",
				"title": "Timed Goals: View UI"
			},
			{
				"command": "timedgoals.showTimer",
				"title": "Timed Goals: Show Timer"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js",
		"build": "npx babel src --out-dir dist & browserify dist/App.js > dist/compiled.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"@babel/cli": "^7.13.14",
		"@babel/core": "^7.13.15",
		"@babel/preset-env": "^7.13.15",
		"@babel/preset-react": "^7.13.13",
		"@babel/register": "^7.13.14",
		"browserify": "^17.0.0",
		"fs": "0.0.1-security",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-redux": "^7.2.3",
		"react-spring": "^9.1.1",
		"redux": "^4.0.5"
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-react"
		]
	}
}
